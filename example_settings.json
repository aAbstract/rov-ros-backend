{
    "networking": {
        "main_tcu": "192.168.122.1",
        "raspi": "192.168.122.16",
        "monitor_tcu": "192.168.122.1"
    },
    "ros": {
        "msg_queue_size": 10,
        "nodes": {
            "main_tcu": [
